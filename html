                        <div rel='1050&1400' class="slide" style="background-image: url(Media/HeavenOnEarth_WireFrames_032023_0310.png);">

                            <div class="caption zwart">
                                <h2></h2>
                            </div>
                        </div>
                    </div>

                </div>
                     
        <a href="#" class="arrow l"></a><a href="#" class="arrow r"></a></div>

        <!--
        This script randomizes anything inside “<div class="image-scroll">”.
        -->

        <script type="text/javascript">

                function shuffleImages() {
                    const container = document.querySelector('.image-scroll');
                    for (let i = container.children.length; i >= 0; i--) {
                        container.appendChild(container.children[Math.random() * i | 0]);
                    }
                }

        shuffleImages();

        </script>

    </body>

</html>
